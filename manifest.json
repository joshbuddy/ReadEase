{
  "manifest_version": 2,
  "name": "ReadEase",
  "version": "1.0",

  "description": "Normalize unicode to simple text",

  "icons": {
    "48": "icons/book128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["norm.js"],
      "run_at": "document_end"
    }
  ]
}